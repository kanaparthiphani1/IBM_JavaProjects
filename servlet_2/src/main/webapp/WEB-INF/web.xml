<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ListLeague</servlet-name>
  	<display-name>ListLeague</display-name>
  	<description></description>
  	<servlet-class>com.example.servlet.ListLeague</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterLeague</servlet-name>
  	<display-name>RegisterLeague</display-name>
  	<description></description>
  	<servlet-class>com.example.servlet.RegisterLeague</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ListLeague</servlet-name>
  	<url-pattern>/list_leagues.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterLeague</servlet-name>
  	<url-pattern>/register_league.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
     <welcome-file>index.html</welcome-file>
   </welcome-file-list>
</web-app>
